<div class="container">
  <h1>Hello <%= current_user.first_name %></h1>
  <h2>Finished a track? Get it off soundcloud and post it here!</h2>
  <p>This well help other users find out what you're about!</p>

  <%#= simple_form_for  do |f| %>
    <%#= f.input :soundcloud_url %>
    <%#= f.collection  %>
    <%#= f.input collection: current_user.tracks, label_method: :name, value_method: :id,label: "Track" %>
    <%#= f.submit %>
  <% #end %>




  <h1>Here are your masterpieces: </h1>
  <iframe width="100%" height="300" scrolling="no" frameborder="no" allow="autoplay" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/632100057&color=%2340096b&auto_play=false&hide_related=false&show_comments=true&show_user=true&show_reposts=false&show_teaser=true&visual=true"></iframe>
  <% @final_tracks.each do |track| %>
    <!-- <%= track.soundcloud_url.html_safe? %> -->
  <% end %>


</div>


